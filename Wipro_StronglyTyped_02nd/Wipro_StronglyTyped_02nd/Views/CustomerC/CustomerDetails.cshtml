@model List<Wipro_StronglyTyped_02nd.Models.Customer>;

<table >
    <thead>
        <tr>
            <th>Customer ID</th>
            <th>Customer Name</th>
            <th>Gender</th>
            <th>Contact</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td>@obj.CustomerID</td>
                <td>@obj.CustomerName</td>
                <td>@obj.CustomerGender</td>
                <td>@obj.CustomerContact</td>
            </tr>
        }
    </tbody>
</table>

